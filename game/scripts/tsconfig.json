    "compilerOptions": {
        "experimentalDecorators": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "alwaysStrict": true,
        "strictNullChecks": true,
        "target": "es6",
        "rootDir": "./vscripts"
    },
    "tstl": {
        "luaTarget": "JIT",
